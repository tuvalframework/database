{
    "name": "@tuval/database",
    "version": "1.0.8",
    "description": "",
    "keywords": [],
    "main": "index.js",
    "module": "index.js",
    "typings": "index.d.ts",
    "files": [
        "index.js",
        "index.d.ts",
        "node/index.js"
    ],
    "author": "tuvalframework <tuvalframework@gmail.com>",
    "bugs": {
        "url": "https://github.com/tuvalframework/core/issues"
    },
    "homepage": "http://tuvalframework.com/",
    "readmeFilename": "README.md",
    "repository": {
        "type": "git",
        "url": "git://github.com/tuvalframework/core.git"
    },
    "license": "MIT",
    "engines": {
        "node": ">=6.0.0"
    },
    "scripts": {
        "build": "webpack --config webpack.config.js"
    },
    "lint-staged": {
        "{src,test}/**/*.ts": [
            "prettier --write",
            "git add"
        ]
    },
    "config": {
        "commitizen": {
            "path": "node_modules/cz-conventional-changelog"
        }
    },
    "jest": {
        "transform": {
            ".(ts|tsx)": "ts-jest"
        },
        "testEnvironment": "node",
        "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "coveragePathIgnorePatterns": [
            "/node_modules/",
            "/test/"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 90,
                "functions": 95,
                "lines": 95,
                "statements": 95
            }
        },
        "collectCoverageFrom": [
            "src/*.{js,ts}"
        ]
    },
    "prettier": {
        "semi": false,
        "singleQuote": true
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "devDependencies": {
        "@types/jest": "^29.5.13",
        "@types/node": "^18.11.7",
        "arraybuffer-loader": "^1.0.8",
        "clean-webpack-plugin": "^4.0.0",
        "copy-webpack-plugin": "^9.0.1",
        "css-loader": "^6.3.0",
        "declaration-bundler-webpack-plugin": "^1.0.3",
        "dts-bundle": "^0.7.3",
        "dts-bundle-generator": "^5.9.0",
        "dts-bundle-webpack": "^1.0.2",
        "express": "^4.21.0",
        "file-loader": "^6.2.0",
        "follow-redirects": "^1.15.1",
        "ifdef-loader": "^2.3.0",
        "jest": "^29.7.0",
        "node-loader": "^2.0.0",
        "rimraf": "^3.0.2",
        "shelljs": "^0.8.4",
        "to-string-loader": "^1.1.6",
        "ts-jest": "^29.2.5",
        "ts-loader": "^9.2.6",
        "ts-node": "^10.9.2",
        "typescript": "^5.6.2",
        "url-loader": "^4.1.1",
        "webpack": "^5.56.1",
        "webpack-cli": "^4.8.0",
        "webpack-conditional-loader": "^1.0.12",
        "webpack-node-externals": "^3.0.0"
    },
    "dependencies": {
        "@tuval/cache": "^1.0.2",
